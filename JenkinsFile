pipeline{
 agent any
   tools{
   maven 'MAVEN_HOME'
   jdk 'jdk'
   }
   stages{
      stage ('Compile stage') { 
		steps{
		    bat 'mvn clean compile'
		}
	}
	 stage('Tesing') { 
            steps {
                bat 'mvn test'
		echo 'in test stage'
            }
        }
	 stage('Deploy') { 
            steps {
                echo 'in deplyment stage1'
            }
        }
    }
}
   
